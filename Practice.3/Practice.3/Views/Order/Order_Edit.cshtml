@model Practice._3.Models.Order

@{
    ViewBag.Title = "Edit Order";
}

<h2>Edit Order</h2>

@using (Html.BeginForm())

{
    <div class="form-group">
        @Html.LabelFor(model => model.shemkvetiID)
        @Html.TextBoxFor(model => model.shemkvetiID, new { @class = "form-control", @readonly = "readonly" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.personaliID)
        @Html.TextBoxFor(model => model.personaliID, new { @class = "form-control" })
    </div>


    <button type="submit" class="btn btn-primary">Update</button>
}